input {

  file {
    path => "/var/log/httpd/*access*log"
    type => "apache-access"
  }

  file {
    path => "/var/log/httpd/*error*log"
    type => "apache-error"
  }
}

output {
  stdout { codec => rubydebug }
  redis { host => "{{ logstash_redis_ip }}" data_type => "list" key => "logstash" }
}
